<!--Login-->

<body>
    <div class="login-container">
        <form class="login-form">
            <h2>Register</h2>
            <label for="username">Username</label>
            <input type="text" id="username" name="" required>
            <label for="email">Email</label>
            <input type="email" id="email" name="" required>
